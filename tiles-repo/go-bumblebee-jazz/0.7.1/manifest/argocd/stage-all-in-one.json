{
    "metadata": {
      "name": "go-bumblebee-jazz-all-in-one",
      "namespace": "argocd"
    },
    "spec": {
      "source": {
        "repoURL": "https://github.com/cc4i/go-bumblebee",
        "path": "manifests/kustomize/staging",
        "targetRevision": "new-combo-jazz"
      },
      "destination": {
        "server": "https://kubernetes.default.svc",
        "namespace": "go-bumblebee"
      },
      "project": "default",
      "syncPolicy": {
        "automated": {
          "prune": true,
          "selfHeal": true
        }
      }
    }
  }